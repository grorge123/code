#include<bits/stdc++.h>
#define IOS ios::sync_with_stdio();cin.tie(0);
using namespace std;
int main(){
    IOS
    int n,m,q,s[1000]={},all[10005]={};
    vector<int> ve[10005];
    cin >> n>>m>>q;
    for(int i=1;i<=n;i++){
        cin >> s[i];
    }
    for(int i=1;i<=m;i++){
        int k;
        cin >> k;
        for(int q=0;q<k;q++){
            int a;
            cin >> a;
            ve[i].push_back(a);
            all[i]+=s[a];
        }
    }
    for(int i=0;i<q;i++){
        string a;
        int b;
        cin >> a>> b;
        if(a=="QUERY"){
            all[b]=0;
            for(int i=0;i<ve[b].size();i++){
                all[b]+=s[ve[b][i]];
            }
            cout << all[b]<<endl;
        }else if(a=="ADD"){
            int c;
            cin >> c;
            for(int i=0;i<ve[b].size();i++){
                s[ve[b][i]]+=c;
            }
        }
    }
}
