#include<cstdio>
#include<bits/stdc++.h>
using namespace std;
#define F first
#define S second
#define pb push_back
bool cmp(pair<int,int> a,pair<int,int> b){
    if(a.F!=b.F){
        return a.F<b.F;
    }else{
        return a.S<b.S;
    }
}
vector<int> ve[100005];
pair<int,int> pa[100005];
int main(){
    int n,m,sq;
    scanf("%d %d",&n,&m);
    sq=sqrt(n);
//    cout << sq<<endl;
    for(int i=0;i<m;i++){
        int a,b;
        scanf("%d %d",&a,&b);
        ve[a].pb(b);
        ve[b].pb(a);
        pa[i]=pair<int,int>(a,b);
    }
    for(int i=0;i<n;i++){
        sort(ve[i].begin(),ve[i].end());
    }
//    sort(pa,pa+m,cmp);
    int ans=0;
    int adj[100005]={};
    vector<pair<int,int> > node;
    for(int i=0;i<n;i++){
        if(!ve[i].size()>=sq){//窮舉所有邊
//            cout <<"i:"<< i<<endl;
            for(int q=0;q<ve[i].size();q++){
//                cout << q<<endl;
                adj[ve[i][q]]=i+1;
            }
            for(int q=0;q<m;q++){
//                cout << adj[pa[q].F]<<" "<<adj[pa[q].S]<<" "<<i+1<<endl;
                if(adj[pa[q].F]==i+1&&adj[pa[q].S]==i+1){
                    ans++;
                }
            }
//            cout <<"ans:"<<ans<<endl;
        }else{//窮舉所有v的邊
//            cout << i<<endl;
            int all=0;
//            pair<int,int> node[100005];
            for(int q=0;q<ve[i].size();q++){
                for(int k=q+1;k<ve[i].size();k++){
                    node.pb(pair<int,int>(ve[i][q],ve[i][k]));
                    all++;
                }
            }
            for(int q=0;q<node.size();q++){
                int l=0,r=ve[node[q].F].size()-1;
                while(l<=r){
                    int m=(l+r)/2;
                    if(ve[node[q].F][m]>node[q].S){
                        r=m-1;
                    }else if(ve[node[q].F][m]<node[q].S){
                        l=m+1;
                    }else{
                        ans++;
//                        cout <<node[i].F<<" "<<node[i].S<<endl;
                        break;
                    }
                }
            }
            node.clear();
        }
    }
//    sort(node.begin(),node.end(),cmp);
//    std::vector<pair<int,int> >::iterator it;//將重複的點刪除
//    it = std::unique (node.begin(), node.end());
//    node.resize( std::distance(node.begin(),it) );
//    it=node.begin();
//    int adj[100005]={};
//    for(int i=0;i<n;i++){
//        if(i<it->F)continue;
//        for(int q=0;q<ve[i].size();q++){
//            adj[ve[i][q]]=i;
//        }
//        for(;it!=node.end()&&it->F<=i;it++){//這個for迴圈註解後就不會tle了
//            if(adj[it->S]==i){
//                ans++;
//            }
//        }
//    }
    printf("%d\n",ans/3);
}
