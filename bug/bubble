#include<stdio.h>
#include<stdlib.h>
int main(){
    int n=0,s[50000]={},all=0;
    while(scanf("%d",&s[n])==1){
        if(s[n]==-1)break;
        if(s[n]>300)all+=s[n];
        n++;
    }
    for(int i=0;i<n;i++){
        for(int q=0;q<n-1;q++){
            if(s[q]>s[q+1]){
                int tmp=s[q+1];
                s[q+1]=s[q];
                s[q]=tmp;
            }
        }
    }
    printf("[");
    for(int i=0;i<n;i++){
        if(i==0)
            printf("%d",s[i]);
        else
            printf(", %d",s[i]);
    }
    printf("]\n");
    printf("[");
    for(int i=n-1;i>=0;i--){
        if(i==n-1)
            printf("%d",s[i]);
        else
            printf(", %d",s[i]);
    }
    printf("]\n");
    printf("%d",all);
    return 0;
}
